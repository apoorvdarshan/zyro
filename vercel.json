{
  "rewrites": [
    { "source": "/api/search", "destination": "/api/search" },
    { "source": "/api/top-headlines", "destination": "/api/top-headlines" },
    { "source": "/api/categories", "destination": "/api/categories" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS" }
      ]
    }
  ]
}
